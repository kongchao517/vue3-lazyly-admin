    "echarts": "^5.4.1",
    "element-plus": "^2.2.32",
    "node-sass": "^8.0.0",
    "pinia": "^2.0.28",
    "postcss-px-to-viewport": "^1.1.1",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "unplugin-auto-import": "^0.15.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "autoprefixer": "^10.4.13",
    "cz-git": "^1.4.1",
    "eslint": "^8.31.0",
